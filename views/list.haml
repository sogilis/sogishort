%table.table.table-striped
  %thead
    %tr
      %th Short link
      %th Full link
      %th Hits
      %th Details
  %tbody
    - links.each do |link|
      %tr
        %td
          %a{:href => "#{base}/#{link[:hash]}"}="#{base}/#{link[:hash]}"
        %td
          %a{:href => link[:url]}=link[:url]
        %td
          %code=link[:hits]
        %td
          %a{:href => "/v/#{link[:hash]}"} ->
